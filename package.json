    "express": "^5.1.0",
    "langchain": "^0.3.27",
    "marked": "^12.0.2",
    "vue": "^3.4.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "electron": "^36.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
